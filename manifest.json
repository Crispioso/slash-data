{
    "manifest_version": 2,
    "name": "slash data",
    "version": "0.2",
    "background": {
        "persistent": false,
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
        "matches": [
            "https://beta.ons.gov.uk/*",
            "https://develop.web.ons.digital/*"
        ],
        "js": [
            "jquery-2.1.4.min.js",
            "slash-data.js"
            ]
        }
    ],
    "commands": {
        "json": {
            "suggested_key": {
                "default": "Ctrl+Shift+U"
            },
            "description": "Toggle page data"
        }
    }
}